[32m+[m[32mconst list = document.createElement('ul');[m
[32m+[m[32mconst info = document.createElement('p');[m
[32m+[m[32mconst html = document.querySelector('html');[m
[32m+[m
[32m+[m[32minfo.textContent = 'Below is a dynamic list. Click anywhere on the page to add a new list item. Click an existing list item to change its text to something else.';[m
[32m+[m
[32m+[m[32mdocument.body.appendChild(info);[m
[32m+[m[32mdocument.body.appendChild(list);[m
[32m+[m
[32m+[m[32mhtml.onclick = function() {[m
[32m+[m[32m  const listItem = document.createElement('li');[m
[32m+[m[32m  const listContent = prompt('What content do you want the list item to have?');[m
[32m+[m[32m  listItem.textContent = listContent;[m
[32m+[m[32m  list.appendChild(listItem);[m
[32m+[m
[32m+[m[32m  listItem.onclick = function(e) {[m
[32m+[m[32m    e.stopPropagation();[m
[32m+[m[32m    const listContent = prompt('Enter new content for your list item');[m
[32m+[m[32m    this.textContent = listContent;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
