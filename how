[32m+[m[32m<!DOCTYPE HTML>[m
[32m+[m[32m<html>[m
[32m+[m[32m    <head>[m
[32m+[m[32m        <style>[m
[32m+[m[32m            .nice{[m
[32m+[m[32m                font-family: sans-serif;[m
[32m+[m[32m            }[m
[32m+[m[32m        </style>[m
[32m+[m[32m    </head>[m
