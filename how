[32m+[m[32m        <p id="myText">My Text</p>[m
[32m+[m[32m        <script>[m
[32m+[m[32m            var el = document.getElementById("myText");[m
[32m+[m[32m            el.classList.add("nice");       // adds the nice class to the paragraph[m
[32m+[m[32m            el.classList.remove("nice");    // removes the nice class from the paragraph[m
[32m+[m[32m            el.classList.toggle("nice");    // adds the nice class to the paragraph again, since it does not currently contain the nice class[m
[32m+[m[41m        [m
[32m+[m[32m            if(el.classList.contains("nice")) {[m
[32m+[m[32m                alert("The paragraph has the nice class");[m
[32m+[m[32m            }[m
[32m+[m[32m        </script>[m
[32m+[m[32m    </body>[m
