[32m+[m[32mh2,[m[41m [m
[32m+[m[32m.special{           /*h2, .special Or use whitespace for more readability*/[m
[32m+[m[32m  color: blue;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mli[class^="a"]{           /*class that start with a*/[m
[32m+[m[32m  font-size: 120%;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mli[class$="a"]{           /*class that end with a*/[m
[32m+[m[32m  background-color: aqua;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mli[class*="a"]{           /*class that contain a*/[m
[32m+[m[32m  color:deeppink;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mli[class~="a"]{           /*class that contain "a" as a single word. eg. "a" or "a b". If "ab" then it doesnt work*/[m
[32m+[m[32m  color:chartreuse;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32marticle p:first-child{   /*it will always target first p of article*/[m
[32m+[m[32m  font-size: 120%;[m
[32m+[m[32m  font-weight: bold;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32marticle p:last-child{   /*it will always target last p of article*/[m
[32m+[m[32m  font-size: 120%;[m
[32m+[m[32m  font-weight: lighter;[m
[32m+[m[32m  color:antiquewhite[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32marticle p:nth-child(2){   /*it will always target 2nd p of article*/[m
[32m+[m[32m  font-size: 120%;[m
[32m+[m[32m  font-weight:lighter;[m
[32m+[m[32m  color:palevioletred;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32marticle p:only-child{   /*it will always target only p of article*/[m
[32m+[m[32m  font-size: 120%;[m
[32m+[m[32m  font-weight:lighter;[m
[32m+[m[32m  color:darkorange;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32minput:invalid{   /*it will always target invalid input*/[m
[32m+[m[32m  font-size: 120%;[m
[32m+[m[32m  font-weight:lighter;[m
[32m+[m[32m  color:darkred;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*User action pseudo-classes*/[m
[32m+[m[32ma:link,[m
[32m+[m[32ma:visited{[m
[32m+[m[32m  color:blue;[m
[32m+[m[32m  font-weight: bold;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32ma:hover{[m
[32m+[m[32m  color:yellowgreen;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*Pseudo-elements*/[m
[32m+[m[32marticle p::first-letter{  /*it will always target the first letter of p in an article. Another example is ::firstline which will always target the first line following the width of the device*/[m
[32m+[m[32m  font-size: 200%;[m
[32m+[m[32m  color: red;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m/*Combining pseudo-classes and pseudo-elements*/[m
[32m+[m[32marticle p:first-child::first-letter{  /*it will always target the first letter of first p in an article*/[m
[32m+[m[32m  font-size: 300%;[m
[32m+[m[32m  color: rgb(170, 255, 101);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*Generating content with ::before and ::after */[m
[32m+[m[32m.box::before{[m
[32m+[m[32m  content: "This is a box";[m
[32m+[m[32m  font-weight: bold;[m
[32m+[m[32m  color: blue;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*A more valid use of these pseudo-elements is to insert an icon, for example the little arrow added in the example below,[m[41m [m
[32m+[m[32mwhich is a visual indicator that we wouldn't want read out by a screen reader:*/[m
[32m+[m
[32m+[m[32m.box1::after{[m
[32m+[m[32m  content:" ‚û•";[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.box2::before{[m
[32m+[m[32m  content: "";[m
[32m+[m[32m  display: block;[m
[32m+[m[32m  width: 100px;[m
[32m+[m[32m  height: 100px;[m
[32m+[m[32m  background-color: rebeccapurple;[m
[32m+[m[32m  border: 1px solid black;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.parent > p {         /*Child Combinator*/[m
[32m+[m[32m  color: red;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*Next-sibling combinator*/[m
[32m+[m[32mh3 + p {[m
[32m+[m[32m  color: blue;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*Subsequent-sibling combinator*/[m
[32m+[m[32mp ~ img {[m
[32m+[m[32m  border:rgb(170, 255, 101) 5px solid;[m
[32m+[m[32m  display:flex;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mh1 {[m
[32m+[m[32m  & ~ p {[m
[32m+[m[32m    /* this is parsed by the browser as h1 ~ p */[m
[32m+[m[32m    font-weight: bold;[m
[32m+[m[32m    background-color: #333;[m
[32m+[m[32m    color: #fff;[m
[32m+[m[32m    padding: 0.5em;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
