[32m+[m[32m<!--Date and time-->[m
[32m+[m[32m<!-- Standard simple date -->[m
[32m+[m[32m<time datetime="2016-01-20">20 January 2016</time><br>[m
[32m+[m[32m<!-- Just year and month -->[m
[32m+[m[32m<time datetime="2016-01">January 2016</time><br>[m
[32m+[m[32m<!-- Just month and day -->[m
[32m+[m[32m<time datetime="01-20">20 January</time><br>[m
[32m+[m[32m<!-- Just time, hours and minutes -->[m
[32m+[m[32m<time datetime="19:30">19:30</time><br>[m
[32m+[m[32m<!-- You can do seconds and milliseconds too! -->[m
[32m+[m[32m<time datetime="19:30:01.856">19:30:01.856</time><br>[m
[32m+[m[32m<!-- Date and time -->[m
[32m+[m[32m<time datetime="2016-01-20T19:30">7.30pm, 20 January 2016</time><br>[m
[32m+[m[32m<!-- Date and time with timezone offset -->[m
[32m+[m[32m<time datetime="2016-01-20T19:30+01:00">[m
[32m+[m[32m  7.30pm, 20 January 2016 is 8.30pm in France[m
[32m+[m[32m</time><br>[m
[32m+[m[32m<!-- Calling out a specific week number -->[m
[32m+[m[32m<time datetime="2016-W04">The fourth week of 2016</time><br>[m
[32m+[m
